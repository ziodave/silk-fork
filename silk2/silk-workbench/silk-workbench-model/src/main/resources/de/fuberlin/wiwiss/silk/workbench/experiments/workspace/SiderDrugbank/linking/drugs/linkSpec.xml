<Interlink id="Drugs">
  <LinkType>&lt;http://www.w3.org/2002/07/owl#sameAs&gt;</LinkType>
  <SourceDataset dataSource="sider" var="a">
    <RestrictTo> ?a ?x ?y . </RestrictTo>
  </SourceDataset>
  <TargetDataset dataSource="drugbank" var="b">
    <RestrictTo> ?b ?x ?y . </RestrictTo>
  </TargetDataset>
  <LinkageRule>
    <Aggregate weight="1" type="max" required="false" id="unnamed_5">
      <Compare weight="10" threshold="0.0" required="false" metric="levenshteinDistance" id="unnamed_1576455">
        <TransformInput function="tokenize" id="unnamed_9">
          <TransformInput function="lowerCase" id="unnamed_1576451">
            <Input path="?a/&lt;http://www4.wiwiss.fu-berlin.de/sider/resource/sider/drugName&gt;" id="unnamed_8"></Input>
          </TransformInput>
          <Param name="regex" value="\s"></Param>
        </TransformInput>
        <TransformInput function="tokenize" id="unnamed_10">
          <TransformInput function="lowerCase" id="unnamed_4">
            <Input path="?b/&lt;http://www.w3.org/2000/01/rdf-schema#label&gt;" id="unnamed_1576453"></Input>
          </TransformInput>
          <Param name="regex" value="\s"></Param>
        </TransformInput>
        <Param name="minChar" value="0"></Param>
        <Param name="maxChar" value="z"></Param>
      </Compare>
      <Compare weight="1" threshold="0.0" required="false" metric="levenshteinDistance" id="unnamed_3">
        <TransformInput function="lowerCase" id="unnamed_6">
          <Input path="?a/&lt;http://www4.wiwiss.fu-berlin.de/sider/resource/sider/drugName&gt;" id="unnamed_2"></Input>
        </TransformInput>
        <TransformInput function="lowerCase" id="unnamed_7">
          <Input path="?b/&lt;http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugbank/synonym&gt;" id="unnamed_1"></Input>
        </TransformInput>
        <Param name="minChar" value="0"></Param>
        <Param name="maxChar" value="z"></Param>
      </Compare>
      <Compare weight="1" threshold="0.0" required="false" metric="levenshteinDistance" id="unnamed_15">
        <TransformInput function="lowerCase" id="unnamed_13">
          <Input path="?a/&lt;http://www4.wiwiss.fu-berlin.de/sider/resource/sider/drugName&gt;" id="unnamed_11"></Input>
        </TransformInput>
        <TransformInput function="lowerCase" id="unnamed_14">
          <Input path="?b/&lt;http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugbank/brandName&gt;" id="unnamed_12"></Input>
        </TransformInput>
        <Param name="minChar" value="0"></Param>
        <Param name="maxChar" value="z"></Param>
      </Compare>
    </Aggregate>
  </LinkageRule>
  <Filter></Filter>
  <Outputs> </Outputs>
</Interlink>
