<SilkFreeTextPreprocessor>
	<Inputs>
		<TrainingDataset id="amazon">
			<Param name="file" value="/Users/Petar/Documents/IdeaProjects/my-silk-fork/silk2/silk-tools/silk-freetext-preprocessing/src/main/resources/de/fuberlin/wiwiss/silk/preprocessing/example/amazon.nt" />
			<Param name="format" value="N-Triple" />
		</TrainingDataset>
		<Dataset id="wdc">
			<Param name="file" value="/Users/Petar/Documents/IdeaProjects/my-silk-fork/silk2/silk-tools/silk-freetext-preprocessing/src/main/resources/de/fuberlin/wiwiss/silk/preprocessing/example/wdc.nt" />
			<Param name="format" value="N-Triple" />
		</Dataset>
	</Inputs>
	<ExtractionJob>
        <Extractor id="BrandExtractor" method="BagOfWords">
            <PropertyForTraining name="Brand" />
            <PropertyToExtractFrom name="title">
                <Transform method="tokenize">
                    <Param name="delimiter" value="\s" />
                </Transform>
            </PropertyToExtractFrom>
        </Extractor>
        <Extractor id="ModelExtractor" method="FeatureValuePairs">
            <PropertyForTraining name="Model" />
            <PropertyToExtractFrom name="title">
                <Transform method="ngrams">
                    <Param name="lowerBound" value="2" />
                    <Param name="upperBound" value="3" />
                    <Transform method="tokenize">
                        <Param name="delimiter" value="\s" />
                    </Transform>
                </Transform>
            </PropertyToExtractFrom>
        </Extractor>
    </ExtractionJob>
	<Outputs>
	</Outputs>
</SilkFreeTextPreprocessor>	